<form class="ore roll-dialog">
 <header class="roll-header taskroll-info">
        <h2 class="roll-name">Rolling {{displayText}} {{pool}}D</h2>
</header>
<div class="taskroll-msg">
    <p style="text-align:center">{{render diceImgs}}</p>
    <p> <strong>Sets: </strong>{{#if (eq sets "")}}None{{else}}{{sets}}{{/if}} | <strong>Loose Dice: </strong>{{loose}}</p>
</div>
</form>